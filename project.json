{
  "name": "barades",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx serve backend",
          "nx serve frontend"
        ],
        "parallel": true
      },
      "description": "Run both frontend and backend in parallel"
    }
  }
}
